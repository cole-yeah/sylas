{
  "root": true,
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parser": "babel-eslint",
  "plugins": ["react", "jsx-a11y", "react-hooks"],
  "rules": {
    "no-unused-vars": "warn",
    "no-unused-expressions":"warn"
  }
}
